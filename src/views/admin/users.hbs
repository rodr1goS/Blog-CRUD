<section class="containerUsers">
    <h2>Usu√°rios cadastrados</h2>
    <div class="users">
        {{#each users}}
            <div class="userData">
                <div class="userInfos">
                    <p id="username">{{dataValues.name}}</p>
                    <p id="email">{{dataValues.email}}</p>
                </div>
                <div class="userActions">
                    <a href="/admin/users/delete/{{dataValues.id}}/"><button id="deleteBtn">Deletar</button></a>
                </div>
            </div>
        {{else}}

        {{/each}}
    </div>
</section>
